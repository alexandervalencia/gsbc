{
  "name": "gsbc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.3.1",
    "cloudinary-react": "1.1.0",
    "date-fns": "1.30.1",
    "firebase": "5.9.3",
    "firebase-admin": "7.2.0",
    "firebase-tools": "6.5.2",
    "formik": "1.5.2",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "normalize.css": "8.0.1",
    "prettier": "1.16.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-modal": "3.8.1",
    "react-rating": "1.6.2",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-router-redux": "4.0.8",
    "react-scripts": "2.1.8",
    "reactstrap": "8.0.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
